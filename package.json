{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@codebakery/origami",
  "version": "3.0.0",
  "description": "Angular + Polymer",
  "keywords": [
    "Angular",
    "Polymer",
    "webcomponents",
    "elements"
  ],
  "contributors": [
    "Liz Mitchell <asyncliz@gmail.com>",
    "Andrew Mitchell <abmitchell77@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hotforfeature/origami.git"
  },
  "homepage": "https://github.com/hotforfeature/origami#readme",
  "bugs": {
    "url": "https://github.com/hotforfeature/origami/issues"
  },
  "bin": {
    "origami": "./bin/origami.js"
  },
  "scripts": {
    "build": "ng-packagr -p package.json",
    "postbuild": "tsc -p bin/",
    "test": "echo \"Error: no test script\" && exit 1",
    "lint": "prettier --write \"*.{js,ts,json,md}\" \"{bin,forms,polyfills,shadycss,src,templates,util}/**/*\"",
    "precommit": "lint-staged",
    "release": "standard-version"
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "flatModuleFile": "origami"
    },
    "whitelistedNonPeerDependencies": [
      "chalk",
      "node-glob",
      "parse5",
      "progress",
      "yargs"
    ]
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "node-glob": "^1.2.0",
    "parse5": "^5.0.0",
    "progress": "^2.0.0",
    "yargs": "^12.0.1"
  },
  "peerDependencies": {
    "@angular/core": "^5.0.0-rc.0 || ^5.0.0 || ^6.0.0-rc.0 || ^6.0.0",
    "@angular/forms": "^5.0.0-rc.0 || ^5.0.0 || ^6.0.0-rc.0 || ^6.0.0",
    "@polymer/polymer": "^3.0.5",
    "@webcomponents/webcomponentsjs": "^2.0.4",
    "tslib": "^1.7.0",
    "typescript": "^2.0.0"
  },
  "devDependencies": {
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@polymer/polymer": "^3.0.5",
    "@types/glob": "^5.0.35",
    "@types/node": "^10.5.5",
    "@types/parse5": "^5.0.0",
    "@types/progress": "^2.0.1",
    "@types/yargs": "^11.1.1",
    "@webcomponents/webcomponentsjs": "^2.0.4",
    "cpr": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "ng-packagr": "^3.0.6",
    "prettier": "^1.14.0",
    "standard-version": "^4.4.0",
    "tsickle": "^0.32.1",
    "tslib": "^1.9.3",
    "typescript": "^2.8.4",
    "zone.js": "^0.8.26"
  },
  "engine": {
    "node": ">=8.0.0"
  }
}
